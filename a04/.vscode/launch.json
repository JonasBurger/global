  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppdbg",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/build/driver",
      "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }
      ],
      "args": [
         "build/naive_pathtracer.algo",
         "configs/a04_pots.json"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}